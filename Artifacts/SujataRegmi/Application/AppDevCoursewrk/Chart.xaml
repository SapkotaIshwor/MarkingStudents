<Window x:Class="AppDevCoursewrk.Chart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppDevCoursewrk"
        xmlns:DV="clr-namespace:System.Windows.Controls.DataVisualization;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        xmlns:DVC="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        
    mc:Ignorable="d"
        Title="Chart" Height="450" Width="800">
    <Grid Margin="0,0,216,37">
        <Grid HorizontalAlignment="Left" Height="377" Margin="10,27,-216,-22" VerticalAlignment="Top" Width="782"/>
        <DVC:Chart Background="AliceBlue" Title="Chart" Margin="0,0,-216,-22">
            <DVC:PieSeries x:Name="Piechart" IndependentValueBinding="{Binding Path=Key}" DependentValueBinding="{Binding Path=Value}">

            </DVC:PieSeries>

        </DVC:Chart>


    </Grid>
</Window>
