<Window x:Class="APPLICATION_DEVELOPMENT.WeeklyStudentReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:APPLICATION_DEVELOPMENT"
        mc:Ignorable="d"
        Title="WeeklyStudentReport" Height="456.051" Width="626.582">
    <Grid Margin="0,0,2,2">
        <Grid.Background>
            <ImageBrush ImageSource="IMAGES/nature.png"/>
        </Grid.Background>
        <DataGrid x:Name="StudentReport" HorizontalAlignment="Left" Height="286" Margin="40,35,0,0" VerticalAlignment="Top" Width="544" ItemsSource="{Binding}"/>
        <Button x:Name="back" Content="Back" HorizontalAlignment="Left" Margin="248,339,0,0" VerticalAlignment="Top" Width="123" FontFamily="Algerian" FontSize="24" Click="back_Click" Height="48" Background="{x:Null}" Foreground="#FF0E0B0B"/>
    </Grid>
</Window>
