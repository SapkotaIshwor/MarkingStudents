<Page x:Class="CW1Appdevelopment.Weeklyreport"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CW1Appdevelopment"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Weeklyreport">

    <Grid>
        <DataGrid x:Name="StudentReport" HorizontalAlignment="Left" Height="369" Margin="0,81,0,0" VerticalAlignment="Top" Width="790" ItemsSource="{Binding}" Background="#FF97BDBF"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="10,390,0,0" VerticalAlignment="Top" Width="115" Height="30" Click="Back" FontSize="20" FontWeight="Bold"/>

    </Grid>
</Page>
