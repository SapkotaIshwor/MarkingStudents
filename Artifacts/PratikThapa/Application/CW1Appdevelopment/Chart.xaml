<Page x:Class="CW1Appdevelopment.Chart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           xmlns:DVC="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        xmlns:local="clr-namespace:CW1Appdevelopment"
        mc:Ignorable="d"
        Title="Chart" Height="450" Width="800">
    <Grid>
        <DVC:Chart>
            <DVC:PieSeries x:Name="chartEnroll" IndependentValueBinding="{Binding Path=Key}" DependentValueBinding="{Binding Path=Value}"></DVC:PieSeries>
        </DVC:Chart>
        <Button Content="Back" HorizontalAlignment="Left" Margin="715,24,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.501,1.442" Click="back"/>
    </Grid>
</Page>
