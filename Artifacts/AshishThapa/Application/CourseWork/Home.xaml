<Window x:Class="StudentInformationSystem.Home"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentInformationSystem"
        mc:Ignorable="d"
        Title="Home" Height="450" Width="800">
    <Grid>
        <Menu IsMainMenu="True">
            <MenuItem x:Name="addStudent" Header="Add Student" FontSize="18" Click="addStudent_Click"></MenuItem>
          
            <MenuItem x:Name="viewGraph" Header="View Graph" Height="30" Width="112" FontSize="18" Click="viewGraph_Click"></MenuItem>
        </Menu>
    </Grid>
</Window>
