<Page x:Class="ADCourseWork.WeeklyStudentReport"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ADCourseWork"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="WeeklyStudentReport">

    <Grid>
        <DataGrid x:Name="StudentReport" HorizontalAlignment="Left" Height="338" Margin="34,38,0,0" VerticalAlignment="Top" Width="652" ItemsSource="{Binding}"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="112,277,0,0" VerticalAlignment="Top" Width="75" FontWeight="Bold" Background="#FFB4B4B4" Click="Button_Back"/>

    </Grid>
</Page>
