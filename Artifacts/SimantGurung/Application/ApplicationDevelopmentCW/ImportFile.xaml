<Window x:Class="ApplicationDevelopmentCW.ImportFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApplicationDevelopmentCW"
        mc:Ignorable="d"
        Title="Import File Window" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Image/backgroundimg.png"/>
        </Grid.Background>
        <Button Content="Import CSV" HorizontalAlignment="Left" Margin="79,90,0,0" VerticalAlignment="Top" FontFamily="Berlin Sans FB Demi" FontSize="20" Width="111" Height="45" Click="btnImportClick"/>
        <DataGrid x:Name="dataImportGrid" Margin="79,149,95,60" ItemsSource="{Binding}" AutoGenerateColumns="True"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="79,32,0,0" VerticalAlignment="Top" FontFamily="Berlin Sans FB Demi" FontSize="20" Width="111" Height="45" Click="btnBackClick"/>
    </Grid>
</Window>
