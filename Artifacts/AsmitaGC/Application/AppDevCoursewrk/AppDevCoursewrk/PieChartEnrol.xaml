<Window x:Class="AppDevCoursewrk.PieChartEnrol"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppDevCoursewrk"
       xmlns:DV="clr-namespace:System.Windows.Controls.DataVisualization;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        xmlns:DVC="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        mc:Ignorable="d"
        Title="PieChartEnrol" Height="664.291" Width="801.546">
    <Grid x:Name="Piechart" HorizontalAlignment="Left" Height="100" Margin="97,172,0,0" VerticalAlignment="Top" Width="425">

        <DVC:Chart Background="AliceBlue" Title="Chart" Margin="0,0,-158,-311">
            <DVC:PieSeries x:Name="PieChart" IndependentValueBinding="{Binding Path=Key}" DependentValueBinding="{Binding Path=Value}">
            </DVC:PieSeries>
        </DVC:Chart>

    </Grid>
</Window>
