<UserControl x:Class="Student_Information_System.dataGrideResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Student_Information_System"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <DataGrid x:Name="dgridstudent" HorizontalAlignment="Left" Height="450" VerticalAlignment="Top" Width="800" Margin="0,0,0,-0.4">
            <DataGrid.Columns>

                <DataGridTextColumn Header="Full Name" Binding="{Binding txtFullname}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
